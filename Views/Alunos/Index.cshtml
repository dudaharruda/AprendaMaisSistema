@model IEnumerable<AprendaMaisSistema.Models.Alunos>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NomeAlunos)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EmailAlunos)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FoneAlunos)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CpfAlunos)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SexoAlunos)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NomeAlunos)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EmailAlunos)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FoneAlunos)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CpfAlunos)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SexoAlunos)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.AlunosId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.AlunosId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.AlunosId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
